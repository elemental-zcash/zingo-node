{
  "name": "@elemental-zcash/zingo-node",
  "version": "0.0.1-beta.1",
  "description": "Node.js bindings for Zingo wallet/SDK",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "exports": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf lib/",
    "build:types": "tsc -d -p tsconfig.json",
    "build:js": "tsc -p tsconfig.json",
    "build": "run-s clean build:js build:types",
    "native:build": "cargo-cp-artifact -a cdylib zingolib-native native/index.node -- cargo build --manifest-path rust/Cargo.toml --message-format=json-render-diagnostics",
    "native:build-release": "npm run native:build -- --release",
    "native:build:linux-x64": "npm run native:build-release -- --target x86_64-unknown-linux-gnu",
    "native:build:linux-x64-musl": "npm run native:build-release -- --target x86_64-unknown-linux-musl",
    "native:build:linux-arm64": "npm run native:build-release -- --target aarch64-unknown-linux-gnu",
    "native:build:macos-x64": "npm run native:build-release -- --target x86_64-apple-darwin",
    "native:build:macos-arm64": "npm run native:build-release -- --target aarch64-apple-darwin",
    "native:build:windows-x64": "npm run native:build-release -- --target x86_64-pc-windows-msvc",
    "native:build:windows-arm64": "npm run native:build-release -- --target aarch64-pc-windows-msvc",
    "install": "node-pre-gyp install --fallback-to-build=false"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "cargo-cp-artifact": "^0.1.8",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2"
  },
  "binary": {
    "module_name": "index",
    "module_path": "./native",
    "host": "https://github.com/elemental-zcash/zingo-node/releases/download/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}-{libc}.tar.gz",
    "remote_path": "{version}"
  }
}
